{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "bundler",
    "module": "es2020",
    "lib": ["es2020", "DOM"],
    "target": "es2020",
    "resolveJsonModule": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "noEmit": true,
    "jsx": "preserve"
  },
  "exclude": ["build", "dist"]
}
