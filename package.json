{
  "name": "svelte-preprocess-react",
  "description": "Seamlessly use React components inside a Svelte app",
  "keywords": [
    "svelte",
    "react",
    "interoperability"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bfanger/svelte-preprocess-react.git"
  },
  "version": "0.16.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "lint": "concurrently -c \"#c596c7\",\"#676778\",\"#3074c0\",\"#7c7cea\" --kill-others-on-fail \"npm:lint:*\"",
    "lint:prettier": "prettier --check \"src/**/*.svelte\"",
    "lint:svelte-check": "svelte-check --fail-on-warnings",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --ext=js,ts,svelte --max-warnings=0 src",
    "format": "eslint --ext=js,ts,svelte --fix src && prettier --write .",
    "test": "concurrently -c \"#fcc72a\",\"#45ba4b\" --kill-others-on-fail \"npm:test:*\"",
    "test:vitest": "vitest run",
    "test:playwright": "playwright test",
    "vitest": "vitest watch",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --max-warnings 0 --no-ignore",
      "sh -c 'tsc -p tsconfig.json --noEmit'"
    ],
    "*.(c)?js": [
      "eslint --max-warnings 0 --no-ignore"
    ],
    "*.svelte": [
      "eslint --max-warnings 0 --no-ignore",
      "svelte-check --fail-on-warnings",
      "prettier --check"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.36.2",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.22.3",
    "@sveltejs/package": "^2.2.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/svelte": "^4.0.3",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-config-triple": "^1.1.3",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.32.4",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.64.1",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.6",
    "svelte2tsx": "^0.6.19",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "magic-string": "^0.30.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "svelte": ">=4.0.0"
  },
  "svelte": "./dist/index.js",
  "files": [
    "./dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./preprocessReact": {
      "types": "./dist/preprocessReact.d.ts",
      "default": "./dist/preprocessReact.js"
    },
    "./react-router": {
      "types": "./dist/react-router/index.d.ts",
      "svelte": "./dist/react-router/index.js",
      "default": "./dist/react-router/index.js"
    }
  },
  "typesVersions": {
    ">4.0": {
      "index.d.ts": [
        "./dist/index.d.ts"
      ],
      "preprocessReact": [
        "./dist/preprocessReact.d.ts"
      ],
      "react-router": [
        "./dist/react-router/index.d.ts"
      ]
    }
  }
}
