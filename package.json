{
  "name": "svelte-preprocess-react",
  "description": "Seamlessly use React components inside a Svelte app",
  "keywords": [
    "svelte",
    "react",
    "interoperability"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bfanger/svelte-preprocess-react.git"
  },
  "version": "0.15.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "node ./src/pre-dev.js && svelte-package && vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "lint": "concurrently -c \"#c596c7\",\"#676778\",\"#3074c0\",\"#7c7cea\" --kill-others-on-fail \"npm:lint:*\"",
    "lint:prettier": "prettier --check --loglevel=warn \"src/**/*.svelte\"",
    "lint:svelte-check": "svelte-check --fail-on-warnings",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --ext=js,ts,svelte --max-warnings=0 src",
    "format": "prettier --write . && eslint --ext=js,ts,svelte --fix src",
    "test": "concurrently -c \"#fcc72a\",\"#45ba4b\" --kill-others-on-fail \"npm:test:*\"",
    "test:vitest": "vitest run",
    "test:playwright": "playwright test",
    "vitest": "vitest watch",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --max-warnings 0 --no-ignore",
      "sh -c 'tsc -p tsconfig.json --noEmit'"
    ],
    "*.(c)?js": [
      "eslint --max-warnings 0 --no-ignore"
    ],
    "*.svelte": [
      "eslint --max-warnings 0 --no-ignore",
      "svelte-check --fail-on-warnings",
      "prettier --check"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.33.0",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.15.9",
    "@sveltejs/package": "1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/svelte": "^3.2.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-react": "3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-triple": "^0.5.8",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte2tsx": "^0.6.11",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "magic-string": "^0.30.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "svelte": ">=3.0.0"
  }
}
