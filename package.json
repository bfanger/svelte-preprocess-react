{
  "name": "svelte-preprocess-react",
  "description": "Seamlessly use React components inside a Svelte app",
  "keywords": [
    "svelte",
    "react",
    "interoperability"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bfanger/svelte-preprocess-react.git"
  },
  "version": "0.17.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "lint": "concurrently -c \"#c596c7\",\"#676778\",\"#7c7cea\" --kill-others-on-fail \"npm:lint:*\"",
    "lint:prettier": "prettier --check \"src/**/*.svelte\"",
    "lint:svelte-check": "svelte-check --fail-on-warnings",
    "lint:eslint": "eslint --ext=js,ts,svelte --max-warnings=0 src",
    "format": "eslint --ext=js,ts,svelte --fix src && prettier --write .",
    "test": "concurrently -c \"#fcc72a\",\"#45ba4b\" --kill-others-on-fail \"npm:test:*\"",
    "test:vitest": "vitest run",
    "test:playwright": "playwright test",
    "vitest:watch": "vitest watch",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,svelte}": [
      "svelte-check --fail-on-warnings"
    ],
    "*.{ts,svelte,js,cjs,mjs}": [
      "eslint --max-warnings 0 --no-ignore"
    ],
    "*.{ts,js,svelte,css,scss,json,html}": [
      "prettier --check"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/package": "^2.2.6",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@testing-library/react": "^14.1.2",
    "@testing-library/svelte": "^4.0.6",
    "@types/node": "^20.11.7",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-triple": "^1.1.5",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-svelte": "^2.35.1",
    "happy-dom": "^13.3.1",
    "husky": "^9.0.6",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.70.0",
    "svelte": "next",
    "svelte-check": "^3.6.3",
    "svelte2tsx": "^0.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "magic-string": "^0.30.5"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "svelte": ">=4.0.0"
  },
  "svelte": "./dist/index.js",
  "files": [
    "./dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./preprocessReact": {
      "types": "./dist/preprocessReact.d.ts",
      "default": "./dist/preprocessReact.js"
    },
    "./react-router": {
      "types": "./dist/react-router/index.d.ts",
      "svelte": "./dist/react-router/index.js",
      "default": "./dist/react-router/index.js"
    }
  },
  "typesVersions": {
    ">4.0": {
      "index.d.ts": [
        "./dist/index.d.ts"
      ],
      "preprocessReact": [
        "./dist/preprocessReact.d.ts"
      ],
      "react-router": [
        "./dist/react-router/index.d.ts"
      ]
    }
  },
  "overrides": {
    "svelte-hmr": {
      "svelte": "next"
    },
    "@testing-library/svelte": {
      "svelte": "next"
    }
  }
}
