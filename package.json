{
  "name": "svelte-preprocess-react",
  "description": "Seamlessly use React components inside a Svelte app",
  "keywords": [
    "svelte",
    "react",
    "interoperability"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bfanger/svelte-preprocess-react.git"
  },
  "version": "2.0.2",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:preprocess": "echo 'src/lib/preprocessReact.js' | entr -rn npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "lint": "concurrently -c \"#c596c7\",\"#676778\",\"#7c7cea\" --kill-others-on-fail \"npm:lint:*\"",
    "lint:prettier": "prettier --check \"src/**/*.svelte\"",
    "lint:svelte-check": "svelte-check --fail-on-warnings",
    "lint:eslint": "eslint --max-warnings=0 src",
    "format": "eslint --fix src && prettier --write .",
    "test": "concurrently -c \"#fcc72a\",\"#45ba4b\" --kill-others-on-fail \"npm:test:*\"",
    "test:vitest": "vitest run",
    "test:playwright": "playwright test",
    "vitest:watch": "vitest watch",
    "preinstall": "npx only-allow pnpm",
    "prepublishOnly": "npm run package",
    "prepare": "husky || true"
  },
  "lint-staged": {
    "*.{ts,svelte}": [
      "svelte-check --fail-on-warnings"
    ],
    "*.{ts,svelte,js,cjs,mjs}": [
      "eslint --max-warnings 0 --no-ignore"
    ],
    "*.{ts,js,svelte,css,scss,json,html}": [
      "prettier --check"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.48.1",
    "@sveltejs/adapter-static": "^3.0.5",
    "@sveltejs/kit": "^2.7.2",
    "@sveltejs/package": "^2.3.5",
    "@sveltejs/vite-plugin-svelte": "4.0.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/svelte": "^5.2.4",
    "@types/node": "^22.7.7",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-svelte": "^2.45.1",
    "globals": "^15.11.0",
    "happy-dom": "^15.7.4",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-youtube": "^10.1.0",
    "svelte": "5.0.2",
    "svelte-check": "^4.0.5",
    "svelte-youtube-lite": "^0.5.1",
    "svelte2tsx": "^0.7.22",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.12"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "svelte": ">=5.0.0"
  },
  "svelte": "./dist/index.js",
  "files": [
    "./dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./preprocessReact": {
      "types": "./dist/preprocessReact.d.ts",
      "default": "./dist/preprocessReact.js"
    },
    "./react-router": {
      "types": "./dist/react-router/index.d.ts",
      "svelte": "./dist/react-router/index.js",
      "default": "./dist/react-router/index.js"
    }
  },
  "typesVersions": {
    ">4.0": {
      "index.d.ts": [
        "./dist/index.d.ts"
      ],
      "preprocessReact": [
        "./dist/preprocessReact.d.ts"
      ],
      "react-router": [
        "./dist/react-router/index.d.ts"
      ]
    }
  },
  "pnpm": {
    "overrides": {
      "svelte": "$svelte",
      "eslint": "$eslint"
    }
  }
}
